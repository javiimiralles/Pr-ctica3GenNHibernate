<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Práctica3GenNHibernate"
		                   namespace="Práctica3GenNHibernate.EN.Práctica3">
		    
		    
			
				<class name="GeneroEN" table="Genero"> 
				  	
						<id name="Id" column="id" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<bag name="Producto" table="[genero_producto]"  >
							<key>
								
									<column name="FK_id_idGenero"/> 
							</key>
      						<many-to-many class="Práctica3GenNHibernate.EN.Práctica3.ProductoEN">
									<column name="FK_id_idProducto"/> 
      						</many-to-many>
    					</bag>
			
		  			
						<bag name="Cliente" >
							<key>
							
								
								<column name="FK_id_idGenero"/>
					      	</key>
							<one-to-many class="Práctica3GenNHibernate.EN.Práctica3.ClienteEN"/>
					    </bag>
					
					
					
					
					
		
		 </class>	
		  
		  <query name="GeneroENReadAllHQL">from GeneroEN</query>
		</hibernate-mapping>
	